{ "manifest_version": 2, 
  "name": "loputoo", 
  "version": "0.1.0", 
  "permissions": ["<all_urls>", "activeTab"], 
  "browser_action": 
   { 
    "default_popup": "index.html",
    "default_icon": "ELLE_letter.png"
  },
  "web_accessible_resources": [ "*.wav" ],
  "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'"
}